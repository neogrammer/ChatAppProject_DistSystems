{
  "name": "chatroomwebview",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG=true webpack serve --config webpack.config.js --mode development",
    "build:debug": "cross-env DEBUG=true webpack --config webpack.config.js --mode development",
    "build:release": "cross-env DEBUG=false webpack --config webpack.config.js --mode production",
    "type-check": "tsc --noEmit",
    "gen:proto": "npx rimraf src/Generated && mkdirp src/Generated && protoc --ts_proto_out=src/Generated --ts_proto_opt=esModuleInterop=true,forceLong=long --proto_path=../../proto ../../proto/chat.proto",
    "prebuild:debug": "npm run gen:proto",
    "prebuild:release": "npm run gen:proto",
    "prestart": "npm run gen:proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.1",
    "@grpc/proto-loader": "^0.8.0",
    "lit": "^3.1.0"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.11.1",
    "@types/node": "^20.12.7",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.9.0",
    "minify-html-literals": "^1.3.5",
    "minify-html-literals-loader": "^1.1.1",
    "mkdirp": "^3.0.1",
    "rimraf": "^6.1.2",
    "ts-loader": "^9.5.1",
    "ts-proto": "^2.8.3",
    "typescript": "^5.4.5",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
